script EventScript_OpponentsRemainingMessage {
	buffernumberstring(0, VAR_TEMP_0)
	switch(var(VAR_TEMP_0)) {
		case 0:
			msgbox(format("There are no opponents remaining in the area."))
		case 1:
			msgbox(format("There is {COLOR RED}1{COLOR DARK_GRAY} opponent remaining in the area."))
		default:
			msgbox(format("There are {COLOR RED}{STR_VAR_1}{COLOR DARK_GRAY} opponents remaining in the area."))
	}
}

