}

script EventScript_TestingLegendBattle {
	lock
	faceplayer
	waitse
	fadeoutbgm(1)
	playmoncry(SPECIES_LUNATONE, CRY_MODE_ENCOUNTER)
	delay(40)
	waitmoncry
	setwildbattle_pbl(SPECIES_LUNATONE, 15, ITEM_LUM_BERRY, NATURE_BOLD, 31, 0, MOVE_MOONLIGHT, MOVE_PSYSHOCK, MOVE_POWER_GEM, MOVE_COSMIC_POWER)
	setflag(FLAG_SYS_NO_RUNNING)
	setflag(FLAG_SYS_NO_CATCHING)
