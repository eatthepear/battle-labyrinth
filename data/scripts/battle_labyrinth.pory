script EventScript_Sign_Notebook {
	msgbox(format("Trainer Tips\pThe {COLOR RED}Notebook{COLOR DARK_GRAY} has a ton of information in it, including mechanics changes from vanilla Pokémon games, upcoming Zones and boss battles, and difficulty info.\pIn addition, any trainer tips sign you read will have its message transcribed in the Notebook."), MSGBOX_SIGN)
	subquestmenu(QUEST_MENU_CHECK_COMPLETE, QUEST_SIGNS, SUB_QUEST_1)
	if (var(VAR_RESULT) == FALSE) {
		completesubquest(QUEST_SIGNS, SUB_QUEST_1)
	}
}

script EventScript_Sign_Porygon {
	if (var(VAR_DIFFICULTY) >= DIFFICULTY_NORMAL) {
		message(format("Trainer Tips\pTo complete a Zone, defeat all Leviathans and find {COLOR RED}Porygon{COLOR DARK_GRAY}."))
	} else {
		message(format("Trainer Tips\pTo complete a Zone, defeat all Leviathans and find {COLOR RED}Porygon{COLOR DARK_GRAY}.\pOn Easy difficulty, you can skip defeating some Leviathans."))
	}
	waitmessage
	subquestmenu(QUEST_MENU_CHECK_COMPLETE, QUEST_SIGNS, SUB_QUEST_2)
	if (var(VAR_RESULT) == FALSE) {
		completesubquest(QUEST_SIGNS, SUB_QUEST_2)
	}
	showmonpic(SPECIES_PORYGON, 10, 3)
	setseenmon(SPECIES_PORYGON)
	waitbuttonpress
	hidemonpic
	closemessage
}

