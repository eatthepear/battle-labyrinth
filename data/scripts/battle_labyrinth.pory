script EventScript_Notebook_ChangesFromVanilla {
	while {
		message(format("Changes from Vanilla open.\nAnything not listed is vanilla."))
		waitmessage
		dynmultipush("Pokémon", 0)
		dynmultipush("Battle", 1)
		dynmultipush("Items", 2)
		dynmultipush("Other", 3)
		dynmultipush("Back", 4)
		dynmultistack(0, 0, FALSE, 6, FALSE, 0, DYN_MULTICHOICE_CB_NONE)
		switch (var(VAR_RESULT)) {
			case 0:
				call(EventScript_Notebook_Pokemon)
			case 1:
				call(EventScript_Notebook_Battle)
			case 2:
				call(EventScript_Notebook_Items)
			case 3:
				call(EventScript_Notebook_Other)
			case 4:
			case MULTI_B_PRESSED:
				return
		}
	}
}

script EventScript_Notebook_Pokemon {
	while {
		message(format("Pokémon Changes open.\nAnything not listed is vanilla."))
		waitmessage
		dynmultipush("Abilities", 1)
		dynmultipush("Evolutions", 2)
		dynmultipush("Learnsets", 3)
		dynmultipush("Friendship", 4)
		dynmultipush("Back", 5)
		dynmultistack(0, 0, FALSE, 6, FALSE, 0, DYN_MULTICHOICE_CB_NONE)
		switch (var(VAR_RESULT)) {
			case 1:
				msgbox(format("Many Pokémon have abilities that replace their default ones.\pThese include weather setting abilities and balance changes."))
				msgbox(format("The next page notes Pokémon with ability changes. Would you like to read it?"), MSGBOX_YESNO)
				if (var(VAR_RESULT) == YES) {
					call(EventScript_Notebook_Abilities)
				}
			case 2:
				msgbox(format("Many Pokémon have evolution methods that are another option from their default or directly replace the default.\pThis includes Day/Night evolutions, Friendship evolutions, gender based evolutions, weak Pokémon evolutions, and hard to achieve evolutions.\pYou can see how to evolve a Pokémon in the Pokédex."))
				msgbox(format("The next page notes Pokémon with major evolution changes. Would you like to read it?"), MSGBOX_YESNO)
				if (var(VAR_RESULT) == YES) {
					call(EventScript_Notebook_Evolutions)
				}
			case 3:
				msgbox(format("Most Pokémon have very similar learnsets as vanilla.\pAll Pokémon have tailored level-up learnsets and may learn moves earlier than they do in vanilla.\pYou can see what moves Pokémon learn in the Pokédex."))
				msgbox(format("The next page notes Pokémon with any learnset changes from vanilla. Would you like to read it?"), MSGBOX_YESNO)
				if (var(VAR_RESULT) == YES) {
					call(EventScript_Notebook_Learnsets)
				}
			case 4:
				msgbox(format("Friendship is always considered maxed out.\pAll friendship evolutions have been changed to level up, so in practice this only affects moves like Return."))
			case 5:
			case MULTI_B_PRESSED:
				return
		}
	}
}

