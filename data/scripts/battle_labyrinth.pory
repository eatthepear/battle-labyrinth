script EventScript_Max {
	lock
	faceplayer
	playbgm(MUS_DP_POKE_MART, FALSE)
	call(EventScript_Shop_Max)
	fadedefaultbgm
	release
}

script EventScript_Shop_Max {
	setspeaker("Max")
	message(format("Hi {PLAYER}! Long time no see!\pWelcome to my TM shop!"))
	waitmessage
	switch(var(VAR_ZONE)) {
		case 2:
			pokemart(Zone2A_Shop_TMShop)
		case 6:
			pokemart(Zone6A_Shop_TMShop)
		case 10:
			pokemart(Zone10A_Shop_TMShop)
		case 18:
			pokemart(Zone18A_Shop_TMShop)
	}
	setspeaker("Max")
	msgbox(format("Goodbye!"), MSGBOX_AUTOCLOSE)
}

mart Zone2A_Shop_TMShop {
	ITEM_TM_HIDDEN_POWER
}

mart Zone6A_Shop_TMShop {
	ITEM_TM_DIG
	ITEM_TM_DIVE
}

mart Zone10A_Shop_TMShop {
	ITEM_TM_TRAILBLAZE
	ITEM_TM_FLAME_CHARGE
	ITEM_TM_CHARGE_BEAM
	ITEM_TM_POWER_UP_PUNCH
}

mart Zone18A_Shop_TMShop {
	ITEM_TM31
	ITEM_TM32
	ITEM_TM33
}

script EventScript_WarpZone0 {
	closemessage
	playmoncry(SPECIES_PORYGON, 0)
	waitmoncry
	clearflag(FLAG_IS_REVISITING_ZONE)
	setvar(VAR_REVISITING_ZONE, 0)
	warp(MAP_ZONE0A, 1, 255, 255)
	waitstate
	releaseall
	end
}

