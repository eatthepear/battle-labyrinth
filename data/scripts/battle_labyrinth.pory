text gText_Nuzlocke_GiftPossible {
	format("On Nuzlocke mode, you will randomly receive one of the possible Pokémon. Would you like to see the list of possible Pokémon?")
}

text gText_Nuzlocke_ReadyReceive {
	format("Are you ready to receive a Pokémon?")
}

script EventScript_NuzlockeVerifyBuy {
	if (flag(FLAG_SETTINGS_NUZLOCKE) == true) {
		msgbox(format("On Nuzlocke mode, you will randomly receive one of the possible Pokémon.\pIs this ok?"), MSGBOX_YESNO)
		if (var(VAR_RESULT) == NO) {
			hidemoneybox
			release
			end
		}
	}
	return
}

script EventScript_NuzlockeVerifyGift {
	if (flag(FLAG_SETTINGS_NUZLOCKE) == true) {
		msgbox(format("On Nuzlocke mode, you will randomly receive one of the possible Pokémon.\pIs this ok?"), MSGBOX_YESNO)
		if (var(VAR_RESULT) == NO) {
			release
			end
		}
	}
	return
}

script EventScript_ChoosePartyMonToSend {
	while {
		msgbox(format("Choose a party Pokémon to send to the PC."))
		special(ChoosePartyMon)
		waitstate
		if (var(VAR_0x8004) != PARTY_NOTHING_CHOSEN) {
			specialvar(VAR_TEMP_3, ScriptGetPartyMonSpecies)
			bufferspeciesname(2, VAR_TEMP_3)
			copyvar(VAR_TEMP_3, VAR_0x8004) // used in SendChosenMonToPC
			msgbox(format("Are you sure you want to send your {STR_VAR_3} to the PC?"), MSGBOX_YESNO)
			if (var(VAR_RESULT) == YES) {
				special(SendChosenMonToPC)
				return
			}
		}
	}
}

script EventScript_PorygonExit {
    lock
	faceplayer
	setvar(VAR_BATTLE_STARTING_STATUS, 0)
	if (flag(FLAG_IS_REVISITING_ZONE)) {
		msgbox(format("Return to the {COLOR GREEN}Sanctuary{COLOR DARK_GRAY}?"), MSGBOX_YESNO)
		if (var(VAR_RESULT) == NO) {
			release
			end
		}
		closemessage
		playmoncry(SPECIES_PORYGON, 0)
		waitmoncry
		warp(MAP_ZONE_SANCTUARY, 255, 48, 12)
		waitstate
		release
		end
	}
	msgbox(format("Proceed to the {COLOR GREEN}Sanctuary{COLOR DARK_GRAY}?"), MSGBOX_YESNO)
	if (var(VAR_RESULT) == YES) {
		closemessage
		playmoncry(SPECIES_PORYGON, 0)
		waitmoncry
		setvar(VAR_GENERIC, 1)
		warp(MAP_ZONE_SANCTUARY, 255, 48, 12)
		waitstate
	}
    release
    end
}

