script EventScript_FoundGrotto {
	msgbox(format("Look!\nYou've found a narrow path.\pWill you follow it?"), MSGBOX_YESNO)
	if (var(VAR_RESULT) == NO) {
		release
		end
	}
	return
}

// Always contains a Pokemon
script EventScript_Grotto0 {
	lock
	call(EventScript_FoundGrotto)
	setvar(VAR_GROTTO_NUMBER, 0)
	warp(MAP_HIDDEN_GROTTO, 255, 9, 15)
	waitstate
	release
	end
}

script EventScript_Grotto1 {
	lock
	call(EventScript_FoundGrotto)
	setvar(VAR_GROTTO_NUMBER, 1)
	warp(MAP_HIDDEN_GROTTO, 255, 9, 15)
	waitstate
	release
	end
}

script EventScript_GrottoPokemon {
	lock
	faceplayer
	waitse
	playmoncry(VAR_GROTTO_SPECIES, CRY_MODE_ENCOUNTER)
	delay(40)
	waitmoncry
	setwildbattle_pbl(VAR_GROTTO_SPECIES, 5, ITEM_NONE, NUM_NATURES, 32, 2)
	setflag(FLAG_SYS_CTRL_OBJ_DELETE)
	dowildbattle
	clearflag(FLAG_SYS_CTRL_OBJ_DELETE)
	setflag(FLAG_TEMP_2)
	goto(Common_EventScript_RemoveStaticPokemon)
}

script EventScript_LeaveGrotto {
	lockall
	switch(var(VAR_GROTTO_NUMBER)) {
		case 0:
			warp(MAP_ZONE_SANCTUARY_QUIET_FIELD, 255, 9, 4)
			waitstate
		case 1:
			warp(MAP_ZONE3A, 255, 37, 6)
			waitstate
	}
	setvar(VAR_GROTTO_NUMBER, 0)
	releaseall
	end
}

script EventScript_RareCandy {
	lock
    faceplayer
    waitse
	removeobject(VAR_LAST_TALKED)
	finditem(ITEM_RARE_CANDY)
	release
	end
}

